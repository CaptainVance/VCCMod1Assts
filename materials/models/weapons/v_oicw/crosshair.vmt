"UnLitGeneric"
{
	"$model" "1"
	"$basetexture" "Models/Weapons/v_oicw/crosshair_3"
	"$translucent" "1"
	"$initialcolor" "{ 37 220 59 }"	// TESLA-X4: This variable stores the default color to revert to
	"$color2" "$initialcolor"

	"Proxies"
	{
		"CustomColor"
		{
			// TESLA-X4: This tells the proxy which variable to place the custom color
			// in (only active if custom colorized materials are enabled)
			"colorVar" "$color2"
			// TESLA-X4: This tells the proxy which variable to retrieve the default
			// color from
			"revertColorVar" "$initialcolor"
		}
	}
}
